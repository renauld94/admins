{
  "agentNetwork": "Neuro AI Ecosystem",
  "models": [
    {
      "title": "Gemma2 9B (Main)",
      "provider": "ollama",
      "model": "gemma2:9b",
      "apiBase": "http://localhost:11434"
    },
    {
      "title": "Gemma2 9B (Chat)",
      "provider": "ollama",
      "model": "gemma2:9b",
      "apiBase": "http://localhost:11434"
    }
  ],
  "tabAutocompleteModel": {
    "title": "Gemma2 9B (Autocomplete)",
    "provider": "ollama",
    "model": "gemma2:9b",
    "apiBase": "http://localhost:11434"
  },

  "modelRoles": {
    "default": "Gemma2 9B (Main)",
    "chat": "Gemma2 9B (Chat)",
    "edit": "Gemma2 9B (Main)",
    "autocomplete": "Gemma2 9B (Autocomplete)",
    "summarize": "Gemma2 9B (Main)",
    "apply": "Gemma2 9B (Main)"
  },

  "communication": {
    "channels": ["unix_socket", "http_loopback", "files"],
    "socketPath": "/run/neuro-agents.sock",
    "httpPort": 5005,
    "contextPath": "./workspace/agents/context"
  },
  "vscodeIntegration": {
    "enableInline": true,
    "aiTriggerPrefixes": ["# ai:", "// ai:"]
  },
  "security": {
    "localOnly": true,
    "requireTls": false,
    "allowedHosts": ["127.0.0.1", "localhost"]
  },

  "experimental": {
    "mcpServers": {
      "ollama-code-assistant": {
        "type": "sse",
        "url": "http://127.0.0.1:5000/mcp/sse"
      }
    }
  }
}
